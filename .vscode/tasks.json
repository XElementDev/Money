{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
	"version": "2.0.0", 
	"tasks": [
		{
			"identifier": "gulp: build", 
			"type": "gulp", 
			"task": "build", 
			"problemMatcher": [ "$tsc" ], 
			"group": {
				"kind": "build", 
				"isDefault": true
			}
		}, 
		{
			"type": "gulp", 
			"task": "clean", 
			"problemMatcher": []
		}, 
		{
			"type": "gulp", 
			"task": "rebuild", 
			"problemMatcher": [ "$tsc" ]
		}
	]
}
